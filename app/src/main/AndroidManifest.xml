<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wehelpyoubook">

    <application
        android:allowBackup="true"
        android:icon="@drawable/avatar_whybook"
        android:label="@string/app_name"
        android:roundIcon="@drawable/avatar_whybook"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wehelpyoubook">
        <activity
            android:name=".HomeActivity"
            android:exported="true"

            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>
        <activity android:name=".SearchableHomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH"/>
            </intent-filter>
            <meta-data android:name="android.app.searchableHome"
                        android:resource="@xml/searchable">
            </meta-data>
        </activity>
    </application>

</manifest>